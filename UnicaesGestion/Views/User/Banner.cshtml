
@{
    ViewBag.Title = "Banner";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!--start layout-->
<!-- Bootstrap core CSS -->
<link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
<link href="~/Content/css/bootstrap-reset.css" rel="stylesheet">
<!--external css-->
<link href="~/Content/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />

<!--right slidebar-->
<link href="~/Content/css/slidebars.css" rel="stylesheet">

<!-- Custom styles for this template -->
<link href="~/Content/css/style.css" rel="stylesheet">
<link href="~/Content/css/style-responsive.css" rel="stylesheet" />
<!--end-->



<!--carousel start-->
<section class="panel">
    <div id="c-slide" class="carousel slide auto panel-body">
        <ol class="carousel-indicators out">
            <li class="active" data-slide-to="0" data-target="#c-slide"></li>
            <li class="" data-slide-to="1" data-target="#c-slide"></li>
            <li class="" data-slide-to="2" data-target="#c-slide"></li>
        </ol>
        <div class="carousel-inner">
            <div class="item active text-center">
                <img src="~/Content/Inicio/img/banner/slider1.png" />
            </div>
            <div class="item text-center">                
                <h3>Massive UI Elements</h3>
                <img src="~/Content/Inicio/img/banner/slider1.png" />
                <small class="text-muted">Fully Responsive</small>
            </div>
            <div class="item text-center">
                <h3>Well Documentation</h3>
                <img src="~/Content/Inicio/img/banner/slider1.png" />
                <small class="text-muted">Easy to Use</small>
            </div>
        </div>
        <a data-slide="prev" href="#c-slide" class="left carousel-control">
            <i class="fa fa-angle-left"></i>
        </a>
        <a data-slide="next" href="#c-slide" class="right carousel-control">
            <i class="fa fa-angle-right"></i>
        </a>
    </div>
</section>
<!--carousel end-->





   
<!-- js placed at the end of the document so the pages load faster -->
<script src="~/Content/js/jquery.js"></script>
<script src="~/Content/js/bootstrap.min.js"></script>
<script class="include" type="text/javascript" src="~/Content/js/jquery.dcjqaccordion.2.7.js"></script>
<script src="~/Content/js/jquery.scrollTo.min.js"></script>
<script src="~/Content/js/slidebars.min.js"></script>
<script src="~/Content/js/jquery.nicescroll.js" type="text/javascript"></script>
<script src="~/Content/js/respond.min.js"></script>

<!--common script for all pages-->
<script src="~/Content/js/common-scripts.js"></script>

