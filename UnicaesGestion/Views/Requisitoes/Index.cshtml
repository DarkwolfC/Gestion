    <script type="text/javascript">

          function mostrar(value_elemento) {
              var total = 8;
              var elemento;
              for (var i = 1; i <= total; i++) {
                  elemento = document.getElementById('option'+i);
                  if (i == value_elemento) {
                      elemento.style.display = "block";
                  } else {
                      elemento.style.display = "none";
                  }
              }
          }
    </script>




}
