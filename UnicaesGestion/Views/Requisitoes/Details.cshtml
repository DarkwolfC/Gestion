@model UnicaesGestion.Requisito

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<!--Corregido-->
<!--main content start-->
<section id="main-content">
    <section class="wrapper site-min-height">
        <!-- page start-->
        <section class="panel">
            <header class="panel-heading">
                Requisito
            </header>

            <div class="panel-body">
                <div class="form">
                    <form class="cmxform form-horizontal tasi-form" method="Post" action="/Requisitoes/Create">

                        <div class="form-group ">
                            <label for="procedimiento" class="control-label col-lg-2">Denominacion</label>
                            <div class="col-lg-10">
                                @Html.DisplayFor(model => model.denominacion)
                            </div>
                        </div>

                        <div class="form-group ">
                            <label for="procedimiento" class="control-label col-lg-2">Tipo de Requisito</label>
                            <div class="col-lg-10">
                                @Html.DisplayFor(model => model.TipoRequisito.tipo)
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="objetivoI" class="control-label col-lg-2">Criterio</label>
                            <div class="col-lg-10">
                                @Html.DisplayFor(model => model.Criterio.criterio1)
                            </div>
                        </div>
                        <div class="form-group ">
                            <label for="objetivoF" class="control-label col-lg-2">Prioridad</label>
                            <div class="col-lg-10">
                                @Html.DisplayFor(model => model.PrioridadRequisito.prioridad)
                            </div>
                        </div>

                        <div class="form-group ">
                            <label for="objetivoF" class="control-label col-lg-2">Perfil de Contratacion</label>
                            <div class="col-lg-10">
                                 @Html.DisplayFor(model => model.PerfilContratacion.analista)
                            </div>
                        </div>

                   

                    </form>
                </div>
            </div>


        </section>
        <!-- page end-->
    </section>
</section>
<!--main content end-->
<!--Start JavaScript-->
@section JavaScript {
    <script src="~/Content/js/ModalFunction.js"></script>

    <script>
        $(document).ready(function () {
            EditableTable.init();
        });
    </script>
}


