@model UnicaesGestion.TipoPuesto

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href="~/Content/assets/toastr-master/toastr.css" rel="stylesheet" />

<!--main content start-->

<section id="main-content">
    <section class="wrapper">
        <!-- page start-->

        <div class="row">
            <section class="panel tasks-widget">
                <header class="panel-heading">
                    Editar el tipo de puesto
                </header>
                <div class="panel-body">

                    <div class="task-content">

                        <div class="panel-body">

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()
                                <div class="form-group">
                                    <button type="submit" class="btn btn-success col-sm-3 col-lg-2" id="agregarTipoPuesto"><i class="fa fa-plus"> Editar</i></button>
                                    <div class="col-lg-9">
                                        <div class="input-group m-bot15">
                                            <span class="input-group-btn">
                                                <button class="btn btn-white"><i class="fa  fa-file-o"></i></button>
                                            </span>
                                            @Html.EditorFor(model => model.tipo, new { htmlAttributes = new { @class = "form-control", id = "tipoPuesto" } })
                                        </div>

                                    </div>
                                </div>
                            }
                        </div>
                        @*@Html.Partial("_PartialList", new ViewDataDictionary(@ViewBag.lista))*@
                        @*@{ Html.RenderAction("Index");}*@

                    </div>
                </div>
            </section>
        </div>

    </section>
   
</section>
<!--main content end-->


<!--Start JavaScript-->
@section JavaScript {

    <script src="~/Content/assets/toastr-master/toastr.js"></script>
    <script src="~/Scripts/views/Validaciones.js"></script>

}
<!--End JavaScript-->