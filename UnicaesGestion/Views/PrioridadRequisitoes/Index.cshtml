@model IEnumerable<UnicaesGestion.PrioridadRequisito>

@{
    ViewBag.Title = "Index";
    Layout = null;
}




<ul id="myUL" class="task-list">
    @foreach (var item in Model)
    {
        <li>

            <div class="task-title">
                <span class="task-title-sp"><a>@Html.DisplayFor(modelItem => item.prioridad)</a></span>

                <div class="pull-right hidden-phone">
                    <button onclick="location.href='@Url.Action("Edit", "PrioridadRequisitoes", new { id=item.id })'" class="btn btn-success btn-xs"><i class=" fa fa-pencil"></i> </button>
                    <button onclick="location.href='@Url.Action("Delete", "PrioridadRequisitoes", new { id=item.id })'" class="btn btn-primary btn-xs"><i class="fa fa-trash-o"></i></button>
                    @*<button  onclick="location.href='#myModal'"   class="btn btn-danger btn-xs"><i class="fa fa-trash-o "></i></button>*@
                    <button class="btn btn-danger btn-xs" data-toggle="modal" href="#myModal" type="button"><i class="fa fa-trash-o "></i></button>
                </div>
            </div>
        </li>

    }
</ul>



